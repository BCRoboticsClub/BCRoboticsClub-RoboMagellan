#include "gps.h"  
#include "ping.h" 
#include "abdrive.h"
//250 full forward
//150 stop
//50 full reverse

int lMotor = 1;
int rMotor = 0;



void driveMotor(int pin, int pulseWidth)
{
  servo_speed(pin, pulseWidth);
}  

int main(){                       

  driveMotor(lMotor, 250);
  driveMotor(rMotor, 250);
 
}
